                        if game:GetService("ReplicatedStorage"):FindFirstChild("Soul Reaper") then
                            toTarget(game:GetService("ReplicatedStorage"):FindFirstChild("Soul Reaper").HumanoidRootPart.CFrame * CFrame.new(2,20,2))
                        else
                        end
                    end
                end)
    
